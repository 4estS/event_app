<div>
  <h1>Log In</h1>
  <h2 class="font-bold text-4xl">Sessions#new</h2>
  <p>Find me in app/views/sessions/new.html.erb</p>
  <%= form_with url: login_path, method: :post do |f| %>
    <div>
      <%= label_tag :email %>
      <%= email_field_tag :email %>
    </div>
    <div>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <%= submit_tag "Log in" %>
  <% end %>
</div>
